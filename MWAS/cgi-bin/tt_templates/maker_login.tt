<div name="other" style="text-align : center;">
<form name="loginform" method="post" action="maker.cgi">
<div class="login" style="text-align : left;">
  <div class="login_header">
    [% message %]
  </div>
  <div class="login_content">

    <span style="padding : 0em 0em 0em 4em"><img src="[% server_opt.html_web %]/images/Maker-Logo.png" height="39" width="45">Maker Web Annotation Service</span>


    <fieldset>
    <label for="authen_username">User Name</label>
    <input id="authen_loginfield" tabindex="1" type="text" name="authen_username" size="20" value="" /><br />
    <label for="authen_password">Password</label>
    <input id="authen_passwordfield" tabindex="2" type="password" name="authen_password" size="20" /><br />
    <input id="authen_rememberuserfield" tabindex="3" type="checkbox" name="authen_rememberuser" value="1" />Remember User Name<br />
    
    <table align=center>
      <td nowrap>
	[% IF server_opt.allow_register %]
	  <img src="[% server_opt.html_web %]/images/icon_user.gif">
	  <a href="maker.cgi?rm=register"> New user registration</a>
 	[% END %]
      </td>
      <td nowrap>
	[% IF server_opt.smtp_server %]
	  <img src="[% server_opt.html_web %]/images/folder_lock.gif">
	  <a href="maker.cgi?rm=forgot_login"> Forgot login?</label></a>
	[% END %]
      </td>
      <td nowrap>
	<img src="[% server_opt.html_web %]/images/icon_info.gif">
	<a href="maker.cgi?rm=help"> Help</a>
      </td>
    </table>
    </fieldset>
  </div>
  
  <div class="login_footer">
    <div class="buttons">
      <input id="authen_loginbutton" tabindex="4" type="submit" name="authen_loginbutton" value="User Sign In" class="button" />
    
      [% IF server_opt.allow_guest %]    
      <input type="Button" name="guest_loginbutton" value="New Guest Account" onclick="doguest()" class="button" />
      [% END %]
    </div>

  </div>
</div>

<input type="hidden" name="destination" value="maker.cgi" />
<input type="hidden" name="rm" value="authen_login" />
