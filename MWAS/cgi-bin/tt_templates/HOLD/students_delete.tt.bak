
[% WRAPPER main.tt %]

[% INCLUDE students_menu.tt %]

<center><h3>Delete Students</h3></center>

<form method="POST" action="/cgi-bin/Course/course_students.cgi">
<input type="hidden" name="rm" value="students_delete_process">

<center>
<table border cellspacing=0 cellpadding=5>
  <tr>
    <th>Delete</th>
    <th>DB ID</th>
    <th>Last Name</th>
    <th>First Name</th>
    <th>E-mail</th>
    <th>uID</th>
  </tr>
  [% FOREACH student = students.sort('last_name') %]
  <tr>
    <td><input type="checkbox" name="student_id" value="[%student.student_id %]"></td>
    <td>[% student.student_id %]</td>
    <td>[% student.last_name %]</td>
    <td>[% student.first_name %]</td>
    <td>[% student.e_mail%]</td>
    <td>[% student.u_id%]</td>
  </tr>
  [% END %]
</table>
<br \>
<input type="submit" value="Delete Checked Students">

[% END %]

