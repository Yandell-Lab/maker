
[% WRAPPER main.tt %]

[% INCLUDE students_menu.tt %]

<center><h3>Add a Student</h3></center>

<form method="POST" action="http://bioinfo.genetics.utah.edu/cgi-bin/Course/course_students.cgi" enctype="application/x-www-form-urlencoded">
<input type="hidden" name="rm" value="students_add_process" />

<table>
  <tr>
    <td>First Name:</td>
    <td><input type="text" name="first_name" size="32" maxlength="20"></td>
  </tr>
  <tr>
    <td>Last Name:</td>
    <td><input type="text" name="last_name" size="32" maxlength="20"></td>
  </tr>
  <tr>
    <td>E-mail:</td>
    <td><input type="text" name="e_mail" size="32" maxlength="20"></td>
  </tr>
  <tr>
    <td>uID (u00123456):</td>
    <td><input type="text" name="u_id" size="9" maxlength="9"></td>
  </tr>
  <tr>
    <td colspan=2 align="center">
      <input type="submit" value="Add Student">
    </td>
  </tr>
</table>
[% END %]

